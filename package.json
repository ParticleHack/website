{
  "name": "particle-hack",
  "version": "0.0.0",
  "description": "A directory and showcase of ways to participate in particle physics",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test:ci": "ember try:each",
    "test:coverage": "cross-env COVERAGE=true ember test",
    "test:ci:coverage": "npm run test:ci && npm run test:coverage",
    "deploy:gh-pages": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -"
  },
  "repository": "https://github.com/ParticleHack/website",
  "author": "Brian Sipple <@Brian_Sipple>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "cross-env": "^3.1.3",
    "ember-ajax": "^2.4.1",
    "ember-cli": "2.12.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-code-coverage": "0.3.8",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "^0.2.8",
    "ember-cli-postcss": "3.0.5",
    "ember-cli-qunit": "^3.1.0",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-svgstore": "0.4.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-composable-helpers": "1.1.2",
    "ember-data": "^2.12.0",
    "ember-export-application-global": "^1.0.5",
    "ember-hook": "^1.4.1",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-responsive": "^2.0.2",
    "ember-source": "~2.12.0",
    "ember-truth-helpers": "1.3.0",
    "liquid-fire": "^0.27.1",
    "loader.js": "^4.2.3",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "private": true
}
